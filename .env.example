# DreamPlanner Frontend — Environment Variables
# Copy this file to .env and fill in real values.

# Backend API base URL (no trailing slash)
VITE_API_BASE=https://api.dreamplanner.com

# WebSocket base URL (no trailing slash)
VITE_WS_BASE=wss://api.dreamplanner.com

# VAPID public key for Web Push notifications
VITE_VAPID_PUBLIC_KEY=

# Stripe publishable key
VITE_STRIPE_PUBLIC_KEY=

# Google OAuth client ID (from Google Cloud Console)
VITE_GOOGLE_CLIENT_ID=

# Apple OAuth service ID (from Apple Developer — Services IDs)
VITE_APPLE_CLIENT_ID=

# TURN server for WebRTC peer connections
VITE_TURN_URL=
VITE_TURN_USERNAME=
VITE_TURN_CREDENTIAL=
